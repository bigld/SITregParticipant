<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns:m="sap.m">
	<Dialog id="exciseFragDialogId" title="Warehouse" showHeader="true"
		class="sapUiSizeCompact">
		<content>
			<Table id="TableId1" includeItemInSelection="true" inset="false"
				growing="true" growingScrollToLoad="true" items="{/results}" mode="SingleSelectMaster"
				growingThreshold="20" noDataText="No Data" updateFinished="onUpdateFinished1"
				class="sapUiSizeCompact">
				<columns>
					<Column hAlign="Left" demandPopin="true" minScreenWidth="Tablet">
						<Text text="Code" />
					</Column>
					<Column hAlign="Left" demandPopin="true" minScreenWidth="Tablet">
						<Text text="Description" />
					</Column>
					<Column hAlign="Left" demandPopin="true" minScreenWidth="Tablet">
						<Text text="" />
					</Column>
				</columns>
				<items>
					<ColumnListItem id="TableTemplate1">
						<cells>
							<Text text="{WHCode}" />
							<Text text="{WHDesc}" />

							<FlexBox alignItems="Start" class="borderLeftOnly" justifyContent="Start">
								<items>
									<Button id="button1" text="" type="Default" icon="sap-icon://print" press="onPrintPress" />
									<Button id="button2" text="" type="Default" icon="sap-icon://print" press="onPrintPress" />
								</items>
							</FlexBox>


						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<buttons>
			<Button text="OK" type="Default" enabled="false" id="FragCloseId"
				press="FragClose" tooltip="OK" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>